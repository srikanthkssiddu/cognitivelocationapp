    "expo": "~46.0.13",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-native": "0.69.6",
    "react-native-maps": "0.31.1",
    "react-native-paper": "^4.12.5",
    "react-native-safe-area-context": "4.3.1",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "~3.15.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
